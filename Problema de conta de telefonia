using System;
using System.Globalization;
class HelloWorld {
  static void Main() {
      
    int minutos;
    double valor;
    
    minutos = int.Parse(Console.ReadLine());
    
    valor = 50.0;
    
    if(minutos > 100){
        valor = valor + (minutos - 100) * 2.0;
    }
    
    Console.WriteLine("O valor a ser pago Ã© R$: " + valor.ToString("F2", CultureInfo.InvariantCulture));
  }
}
